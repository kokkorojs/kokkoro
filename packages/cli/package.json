{
  "name": "@kokkoro/cli",
  "version": "2.0.1",
  "description": "Cli tool for kokkoro.",
  "engines": {
    "node": ">=20.6.0"
  },
  "files": [
    "lib",
    "template"
  ],
  "type": "module",
  "bin": {
    "kokkoro": "lib/index.js"
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "build:watch": "tsc && (concurrently \"tsc -w\" \"tsc-alias -w\")"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kokkorojs/kokkoro.git"
  },
  "keywords": [
    "bot",
    "kokkoro",
    "qq"
  ],
  "author": "Yuki <admin@yuki.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kokkorojs/issues"
  },
  "homepage": "https://kokkoro.js.org",
  "dependencies": {
    "commander": "^11.1.0",
    "ora": "^7.0.1",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@kokkoro/core": "workspace:^",
    "@kokkoro/utils": "workspace:^",
    "@types/prompts": "^2.4.9"
  }
}
