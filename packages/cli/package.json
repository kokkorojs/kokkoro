{
  "name": "@kokkoro/cli",
  "version": "2.0.7",
  "description": "Cli tool for kokkoro.",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "lib",
    "templates"
  ],
  "type": "module",
  "bin": {
    "kokkoro": "bin/kokkoro.js"
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "build:watch": "tsc && (concurrently \"tsc -w\" \"tsc-alias -w\")"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kokkorojs/kokkoro.git"
  },
  "keywords": [
    "bot",
    "kokkoro",
    "qq"
  ],
  "author": "Yuki <admin@yuki.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kokkorojs/issues"
  },
  "homepage": "https://kokkoro.js.org",
  "dependencies": {
    "commander": "^11.1.0",
    "enquirer": "^2.4.1"
  }
}
